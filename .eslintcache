        </MealItem>);\n\n\n    return (\n        <section className={styled.meals}>\n            <Card>\n                <ul>\n                    {mealsList}\n                </ul>\n            </Card>\n        </section>\n\n\n    );\n\n}\n\nexport default AvailableMeals;","/var/www/html/react-food-order-app/src/components/UI/Card/Card.js",[],"/var/www/html/react-food-order-app/src/components/Meals/MealItem/MealItem.js",[],"/var/www/html/react-food-order-app/src/components/Meals/MealItem/MealItemForm.js",[],"/var/www/html/react-food-order-app/src/components/UI/Input/Input.js",[],"/var/www/html/react-food-order-app/src/components/Cart/Cart.js",["88"],"import React , {useContext , useState} from  'react';\n\nimport Modal from '../UI/Modal/Modal'\nimport  styled from './Cart.module.css';\n\nimport CartContext  from '../../store/cart-context'\nimport CartItem from \"./CartItem\";\nimport Checkout from \"./Checkout\";\n\nconst Cart = (props) => {\n    const [isCheckout, setIsCheckout] = useState(false);\n    const [isSubmitting , setIsSubmitting] = useState(false)\n    const [didSumbit , setDidSubmit] = useState(false)\n    const cartCtx = useContext(CartContext);\n\n    const  totalAmount = `$${cartCtx.totalAmount.toFixed(2)}`;\n\n    const hasItems = cartCtx.items.length > 0;\n\n    const cartItemRemoveHandler = (id) => {\n        cartCtx.removeItem(id);\n\n    }\n\n    const cartItemAddHandler = (item) => {\n        cartCtx.addItem({...item, amount: 1});\n    }\n\n    const orderHandler = () => {\n        setIsCheckout(true);\n\n    }\n\n    const submitOrderHandler = async (userData) => {\n        console.log(userData);\n        setIsSubmitting(true);\n         await fetch('http://127.0.0.1/react-food-order-app/setOrder.php' , {\n            method: 'POST',\n            body: JSON.stringify({\n                    user: userData,\n                    orderItem:cartCtx.items\n                })\n\n        })\n        setIsSubmitting(false);\n        setIsSubmitting(true);\n        cartCtx.clearCart();\n\n    }\n\n\n    const modalActions =   <div className={styled.actions}>\n        <button className={styled['button--alt']} onClick={props.onClose}>Close</button>\n        {hasItems && <button className={styled.button} onClick={orderHandler}>Order</button>}\n    </div>;\n\n    const isSubmittingModalContent = <p> Sending order data...</p>\n\n    const didSubmitModalContent = <p> Successfuly sent the order</p>\n    const  cartItems =\n        <ul className={styled['cart-items']}>\n            {cartCtx.items.map((item) =>\n                <CartItem\n                    key={item.id}\n                    name={item.name}\n                    description={item.description}\n                    amount={item.amount}\n                    price={item.price}\n                    onRemove={cartItemRemoveHandler.bind(null,item.id)}\n                    onAdd={cartItemAddHandler.bind(null,item)}>\n                </CartItem>)}</ul>;\n\n    const cartModalContent = (\n        <React.Fragment>\n            {cartItems}\n            <div className={styled.total}>\n                <span>Total Amount</span>\n                <span>{totalAmount}</span>\n            </div>\n            {isCheckout && <Checkout onConfirm={submitOrderHandler} onCancel={props.onClose}/>}\n            {!isCheckout && modalActions}\n        </React.Fragment>\n    )\n\n\n\n\n    return (\n        <Modal onClose={props.onClose}>\n            {!isSubmitting && !didSumbit && cartModalContent}\n            {isSubmitting && isSubmittingModalContent}\n            {!isSubmitting && didSumbit && didSubmitModalContent}\n\n        </Modal>\n\n    )\n\n\n}\n\nexport default Cart;","/var/www/html/react-food-order-app/src/components/UI/Modal/Modal.js",[],"/var/www/html/react-food-order-app/src/store/CartProvider.js",[],"/var/www/html/react-food-order-app/src/store/cart-context.js",[],"/var/www/html/react-food-order-app/src/components/Cart/CartItem.js",[],"/var/www/html/react-food-order-